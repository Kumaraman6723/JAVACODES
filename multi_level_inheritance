//PROGRAM ON MULTI LEVEL INHERITANCE 
public class X {
    public void methodX()
    {
        System.out.println("class X method");
    }
}
class Y extends X{
    public void methodY()
    {
        System.out.println("class y method");
    }
}
class Z extends Y{
    public void methodZ()
    {
        System.out.println("class z method");
    }

    public static void main(String[] args) {
        Z obj = new Z();
        obj.methodZ();
        obj.methodX();
        obj.methodY();

    }}
